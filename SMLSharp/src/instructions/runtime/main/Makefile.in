srcdir = @srcdir@
builddir = @builddir@
top_srcdir = @top_srcdir@
top_builddir = @top_builddir@

include @top_builddir@/commonrule

VPATH = @srcdir@

SOURCES = Instructions.cc

LOCAL_DEFS = $(RUNTIME_DEFS)
LOCAL_CPPFLAGS = $(RUNTIME_CPPFLAGS)

OBJECTS = $(SOURCES:.cc=.$(OBJEXT))

TARGETS = libimlinstructions.$(LIBEXT)

all: $(TARGETS)

clean:
	-rm -f $(OBJECTS) $(TARGETS)

libimlinstructions.$(LIBEXT): $(OBJECTS)
	$(AR) rcu $@ $(OBJECTS)
	-$(RANLIB) $@

##---- dependency (automatically generated) ----
Instructions.$(OBJEXT): Instructions.cc Instructions.hh \
  ../../../../src/runtime/runtime-core/main/SystemDef.hh \
  ../../../.././config.h
