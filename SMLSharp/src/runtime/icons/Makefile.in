srcdir = @srcdir@
builddir = @builddir@
top_srcdir = @top_srcdir@
top_builddir = @top_builddir@

include @top_builddir@/commonrule

VPATH = @srcdir@

all: all_$(RESOURCE_TYPE)

all_: # dummy

all_rc: icons.$(OBJEXT)

icons.$(OBJEXT): icons.rc
	$(WINDRES) -I$(srcdir) -i $(srcdir)/icons.rc -O coff -o $(builddir)/$@

clean:
	rm -f icons.$(OBJEXT)
