structure FormatExpression{
 type priority
 type assocDirection
 type assoc
 type expression
 val isHigherThan
 val assocToString
 val priorityToString
 val toString
 val parse
}
structure PrinterParameter{
 type parameter
 val defaultNewline
 val defaultSpace
 val defaultColumns
 val defaultGuardLeft
 val defaultGuardRight
}
structure PrettyPrinter{
 structure FE{
  type priority => structure FormatExpression
  type assocDirection => structure FormatExpression
  type assoc => structure FormatExpression
  type expression => structure FormatExpression
  val isHigherThan
  val assocToString
  val priorityToString
  val toString
  val parse
 }
 structure PP{
  type parameter => structure PrinterParameter
  val defaultNewline
  val defaultSpace
  val defaultColumns
  val defaultGuardLeft
  val defaultGuardRight
 }
 type environmentEntry
 type environment
 type symbol
 val sort
 val format
 exception Fail
 exception UnMatchEndOfIndent
 exception IndentUnderFlow
}
structure PreProcessor{
 structure FE{
  type priority => structure FormatExpression
  type assocDirection => structure FormatExpression
  type assoc => structure FormatExpression
  type expression => structure FormatExpression
  val isHigherThan
  val assocToString
  val priorityToString
  val toString
  val parse
 }
 structure PP{
  structure FE => structure PrettyPrinter . structure FE
  structure PP => structure PrettyPrinter . structure PP
  type environmentEntry => structure PrettyPrinter
  type environment => structure PrettyPrinter
  type symbol => structure PrettyPrinter
  val sort
  val format
  exception Fail
  exception UnMatchEndOfIndent
  exception IndentUnderFlow
 }
 structure Param{
  type parameter => structure PrinterParameter
  val defaultNewline
  val defaultSpace
  val defaultColumns
  val defaultGuardLeft
  val defaultGuardRight
 }
 structure Environment{
  type entry
  type environment
  val create
  val getEntry
  val map
  val removeEntry
 }
 type parameter
 type context
 val calculate
 val removeAssoc
 val preProcess
 exception Fail
 exception UnMatchEndOfIndent
}
structure BasicFormatters{
 structure FE{
  type priority => structure FormatExpression
  type assocDirection => structure FormatExpression
  type assoc => structure FormatExpression
  type expression => structure FormatExpression
  val isHigherThan
  val assocToString
  val priorityToString
  val toString
  val parse
 }
 type expression
 type formatter
 val format_unit
 val format_int
 val format_word
 val format_real
 val format_char
 val format_string
 val format_substring
 val format_exn_Ref
 val format_exn
 val format_array
 val format_vector
 val format_ref
 val format_bool
 val format_option
 val format_order
 val format_list
}
structure SMLFormat{
 structure FormatExpression{
  type priority => structure FormatExpression
  type assocDirection => structure FormatExpression
  type assoc => structure FormatExpression
  type expression => structure FormatExpression
  val isHigherThan
  val assocToString
  val priorityToString
  val toString
  val parse
 }
 structure PrinterParameter{
  type parameter => structure PrinterParameter
  val defaultNewline
  val defaultSpace
  val defaultColumns
  val defaultGuardLeft
  val defaultGuardRight
 }
 structure BasicFormatters{
  structure FE => structure BasicFormatters . structure FE
  type expression => structure BasicFormatters
  type formatter => structure BasicFormatters
  val format_unit
  val format_int
  val format_word
  val format_real
  val format_char
  val format_string
  val format_substring
  val format_exn_Ref
  val format_exn
  val format_array
  val format_vector
  val format_ref
  val format_bool
  val format_option
  val format_order
  val format_list
 }
 type parameter
 val prettyPrint
 exception Fail
}
signature FORMAT_EXPRESSION{
 type priority
 type assocDirection
 type assoc
 type expression
 val isHigherThan
 val assocToString
 val priorityToString
 val toString
 val parse
}
signature PRINTER_PARAMETER{
 type parameter
 val defaultNewline
 val defaultSpace
 val defaultColumns
 val defaultGuardLeft
 val defaultGuardRight
}
signature PRETTYPRINTER{
 type environmentEntry
 type environment
 type symbol
 val format
 exception Fail
}
signature PREPROCESSOR{
 val preProcess
 exception Fail
}
signature BASIC_FORMATTERS{
 type expression
 type formatter
 val format_unit
 val format_int
 val format_word
 val format_real
 val format_char
 val format_string
 val format_substring
 val format_exn
 val format_exn_Ref
 val format_array
 val format_vector
 val format_ref
 val format_bool
 val format_option
 val format_order
 val format_list
}
signature SMLFORMAT{
 structure FormatExpression{
  type priority => signature FORMAT_EXPRESSION
  type assocDirection => signature FORMAT_EXPRESSION
  type assoc => signature FORMAT_EXPRESSION
  type expression => signature FORMAT_EXPRESSION
  val isHigherThan
  val assocToString
  val priorityToString
  val toString
  val parse
 }
 structure PrinterParameter{
  type parameter => signature PRINTER_PARAMETER
  val defaultNewline
  val defaultSpace
  val defaultColumns
  val defaultGuardLeft
  val defaultGuardRight
 }
 structure BasicFormatters{
  type expression => signature BASIC_FORMATTERS
  type formatter => signature BASIC_FORMATTERS
  val format_unit
  val format_int
  val format_word
  val format_real
  val format_char
  val format_string
  val format_substring
  val format_exn
  val format_exn_Ref
  val format_array
  val format_vector
  val format_ref
  val format_bool
  val format_option
  val format_order
  val format_list
 }
 type parameter
 val prettyPrint
 exception Fail
}
